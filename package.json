{
  "name": "TLick-The-Dog-Log",
  "version": "1.0.4",
  "private": true,
  "description": "舔狗日志",
  "templateInfo": {
    "name": "redux",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "gc": "git-cz",
    "fix": "eslint src --ext .tsx,.ts --color --fix"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@tarojs/components": "3.0.14",
    "@tarojs/react": "3.0.14",
    "@tarojs/runtime": "3.0.14",
    "@tarojs/taro": "^3.0.14",
    "classnames": "^2.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@tarojs/cli": "^3.5.11",
    "@tarojs/mini-runner": "3.0.14",
    "@tarojs/plugin-mock": "^0.0.6",
    "@tarojs/webpack-runner": "3.5.11",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.52",
    "@types/react-redux": "^7.1.9",
    "@types/redux-actions": "^2.6.1",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-preset-taro": "3.0.14",
    "commitizen": "^4.2.1",
    "eslint": "^7.11.0",
    "eslint-config-taro": "3.0.14",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "stylelint": "13.7.2",
    "typescript": "^4.0.3",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run fix",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
